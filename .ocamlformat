break-cases = toplevel
break-collection-expressions = wrap
break-fun-decl = smart
break-infix = wrap
break-infix-before-func = true
break-separators = after
break-sequences = true
break-string-literals = wrap
break-struct = natural
cases-exp-indent = 2
doc-comments = after
doc-comments-padding = 2
doc-comments-tag-only = default
escape-chars = preserve
escape-strings = preserve
extension-sugar = preserve
field-space = tight
if-then-else = keyword-first
indicate-multiline-delimiters = true
indicate-nested-or-patterns = space
infix-precedence = indent
leading-nested-match-parens = true
let-and = compact
let-binding-spacing = compact
let-open = short
margin = 100
module-item-spacing = sparse
ocp-indent-compat = true
parens-ite = false
parens-tuple = always
parens-tuple-patterns = multi-line-only
parse-docstrings = true
sequence-style = separator
single-case = sparse
space-around-collection-expressions = false
type-decl = compact
wrap-comments = false
wrap-fun-args = false
